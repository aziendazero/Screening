    <Attr Name="_isCodegen" Value="true"/>
  </DesignTime>
  <SQLQuery><![CDATA[SELECT  SO_INVITO_BCK.IDINVITO,            
	SO_INVITO_BCK.CODTS,       
        SO_INVITO_BCK.DTCANC,       
        SO_INVITO_BCK.OPCANC,       
	SO_INVITO_BCK.DTAPP,            
	SO_INVITO_BCK.ROUNDCOMUNE,            
	SO_INVITO_BCK.ROUNDINDIV,            
	SO_INVITO_BCK.ROUNDINVITI, 
        SO_INVITO_BCK.DTCREAZIONE, 
        SO_INVITO_BCK.OPINSERIMENTO, 
        SO_INVITO_BCK.DTULTIMAMOD, 
        SO_INVITO_BCK.OPMODIFICA, 
	SO_CNF_TPINVITO.IDTPINVITO || SO_CNF_TPINVITO.DESCRBREVE TPINVITO,            
	SO_CNF_ESITOINVITO.DESCRBREVE DESC_ESITO,            
	SO_CENTRO_PREL_REF.DESCRBREVE DESC_CPREL,            
	SO_INVITO_BCK.NOTEINVITO,            
	SO_INVITO_BCK.ULSS,            
	SO_INVITO_BCK.TPSCR,            
	SO_INVITO_BCK.ORAAPP,            
	SO_INVITO_BCK.MINAPP,            
	SO_INVITO_BCK.ATTIVO,            
	SO_INVITO_BCK.LIVELLO,            
	DECODE(SO_INVITO_BCK.LIVELLO, 2, SO_ACC_CITO2LIV_BCK.COD_RICHIESTA, SO_ACC_CITO1LIV_BCK.COD_RICHIESTA) RICHIESTA_CI,            
	DECODE(SO_INVITO_BCK.LIVELLO, 2, SO_ACC_MAMMO2LIV_BCK.COD_RICHIESTA, SO_ACC_MAMMO1LIV_BCK.COD_RICHIESTA) RICHIESTA_MA,            
	DECODE(SO_INVITO_BCK.LIVELLO, 2, SO_ACC_COLON2LIV_BCK.COD_RICHIESTA, SO_ACC_COLON1LIV_BCK.COD_RICHIESTA) RICHIESTA_CO,            
  	DECODE(SO_INVITO_BCK.LIVELLO, 2, SO_ACC_PFAS2LIV_BCK.COD_RICHIESTA, SO_ACC_PFAS1LIV_BCK.COD_RICHIESTA) RICHIESTA_PF,            
  	DECODE(SO_INVITO_BCK.LIVELLO, 2, SO_ACC_PFAS2LIV_BCK.COD_CAMPIONE, SO_ACC_PFAS1LIV_BCK.COD_CAMPIONE) CAMPIONE_PF,            
	ROUNDINDIV_HPV            
FROM            
	SO_INVITO_BCK,            
	SO_CNF_TPINVITO,            
	SO_CNF_ESITOINVITO,            
	SO_CENTRO_PREL_REF,            
	SO_ACC_CITO1LIV_BCK,            
	SO_ACC_MAMMO1LIV_BCK,            
	SO_ACC_COLON1LIV_BCK,            
	SO_ACC_CITO2LIV_BCK,            
	SO_ACC_MAMMO2LIV_BCK,            
	SO_ACC_COLON2LIV_BCK,            
  	SO_ACC_PFAS1LIV_BCK,            
  	SO_ACC_PFAS2LIV_BCK            
WHERE            
	SO_CNF_TPINVITO.IDTPINVITO(+) = SO_INVITO_BCK.IDTPINVITO            
	AND SO_CNF_TPINVITO.ULSS(+) = SO_INVITO_BCK.ULSS            
	AND SO_CNF_TPINVITO.TPSCR(+) = SO_INVITO_BCK.TPSCR            
	AND SO_CNF_ESITOINVITO.CODESITOINVITO(+) = SO_INVITO_BCK.CODESITOINVITO            
	AND SO_CNF_ESITOINVITO.LIVESITO(+) = SO_INVITO_BCK.LIVESITO            
	AND SO_CNF_ESITOINVITO.ULSS(+) = SO_INVITO_BCK.ULSS            
	AND SO_CNF_ESITOINVITO.TPSCR(+) = SO_INVITO_BCK.TPSCR            
	AND SO_CENTRO_PREL_REF.IDCENTRO(+) = SO_INVITO_BCK.IDCENTROPRELIEVO            
	AND SO_INVITO_BCK.IDINVITO = SO_ACC_CITO1LIV_BCK.IDINVITO(+)            
	AND SO_INVITO_BCK.IDINVITO = SO_ACC_MAMMO1LIV_BCK.IDINVITO(+)            
	AND SO_INVITO_BCK.IDINVITO = SO_ACC_COLON1LIV_BCK.IDINVITO(+)            
  	AND SO_INVITO_BCK.IDINVITO = SO_ACC_PFAS1LIV_BCK.IDINVITO(+)            
	AND SO_INVITO_BCK.IDINVITO = SO_ACC_CITO2LIV_BCK.IDINVITO(+)            
	AND SO_INVITO_BCK.IDINVITO = SO_ACC_MAMMO2LIV_BCK.IDINVITO(+)            
	AND SO_INVITO_BCK.IDINVITO = SO_ACC_COLON2LIV_BCK.IDINVITO(+)            
  	AND SO_INVITO_BCK.IDINVITO = SO_ACC_PFAS2LIV_BCK.IDINVITO(+)            
	AND SO_INVITO_BCK.ULSS = :1            
	AND SO_INVITO_BCK.TPSCR = :2            
	AND SO_INVITO_BCK.CODTS = :3]]></SQLQuery>
