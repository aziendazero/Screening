  <SQLQuery><![CDATA[SELECT 
	Sogg_SoSoggetto.CODTS,  
	Sogg_SoSoggetto.CODANAGREG,  
	Sogg_SoSoggetto.CODICEREGMEDICO,  
	Sogg_SoSoggetto.IDINTERNO,  
	Sogg_SoSoggetto.CF,  
	Sogg_SoSoggetto.COGNOME,  
	Sogg_SoSoggetto.NOME,   
	Sogg_SoSoggetto.DATA_NASCITA,  
	Sogg_SoSoggetto.SESSO,  
	Sogg_SoSoggetto.ULSS,  
	SO_CNF_ANAG_REG.SIGLA, 
	SO_CNF_ANAG_REG.DESCRIZIONE DESCODANAG, 
	SO_CNF_ANAG_REG.SIGLA || ' - ' || SO_CNF_ANAG_REG.DESCRIZIONE CODANAGFULL, 
	SO_MEDICO.COGNOME COGNMED, 
	SO_MEDICO.NOME NOMEMED, 
  FUN_CODTS_TROVA_DOC(Sogg_SoSoggetto.CODTS,Sogg_SoSoggetto.ULSS,'TS') DOC_IDENT 
FROM 
	SO_SOGGETTO Sogg_SoSoggetto, 
	SO_CNF_ANAG_REG, 
	SO_MEDICO 
WHERE 
	SO_CNF_ANAG_REG.CODANAGREG = Sogg_SoSoggetto.CODANAGREG 
	AND SO_MEDICO.CODICEREGMEDICO (+) = Sogg_SoSoggetto.CODICEREGMEDICO 
	AND SO_MEDICO.ULSS (+) = Sogg_SoSoggetto.ULSS]]></SQLQuery>
