  <SQLQuery><![CDATA[SELECT Acc_SoInvito.IDINVITO,        
       Acc_SoInvito.CODTS,        
       Acc_SoInvito.IDTPINVITO,        
       Acc_SoInvito.CODESITOINVITO,        
       Acc_SoInvito.LIVESITO,        
       Acc_SoInvito.ATTIVO,        
       Acc_SoInvito.IDCENTROPRELIEVO,        
       Acc_SoInvito.IDCENTRORICHIAMO,        
       Acc_SoInvito.IDCENTROREF1LIV,        
       Acc_SoInvito.IDCENTROREF2LIV,        
       Acc_SoInvito.IDAPP,        
       Acc_SoInvito.DTAPP,        
       Acc_SoInvito.ORAAPP,        
       Acc_SoInvito.MINAPP,        
       Acc_SoInvito.DTAPPINIZIALE,        
       Acc_SoInvito.CONSENSO,        
       Acc_SoInvito.DTESAMERECENTE,        
       Acc_SoInvito.DTRICHIAMO,        
       Acc_SoInvito.DTINVIORICHIAMO,        
       Acc_SoInvito.OPMEDICO,        
       Acc_SoInvito.DTINVIOSOLLECITO,        
       Acc_SoInvito.ROUNDCOMUNE,        
       Acc_SoInvito.ROUNDINDIV,        
       Acc_SoInvito.VOLONTARIA,        
       Acc_SoInvito.DTBLOCCO,        
       Acc_SoInvito.CHIUSURAROUNDINDIV,        
       Acc_SoInvito.DTULTIMAMOD,        
       Acc_SoInvito.DTCREAZIONE,        
       Acc_SoInvito.OPINSERIMENTO,        
       Acc_SoInvito.DTMODESCLUSIONE,        
       Acc_SoInvito.OPMODIFICA,        
       Acc_SoInvito.FUORIFASCIA,        
       Acc_SoInvito.NOTEINVITO,        
       Acc_SoInvito.ULSS,        
       Acc_SoInvito.TPSCR,        
       Acc_SoInvito.ROUNDINVITI,        
       Acc_SoInvito.TPRICHIAMO,        
       Acc_SoInvito.LIVELLO,       
       Acc_SoInvito.IDOSTETRICA,       
       Acc_SoInvito.DTAPP + (NVL(Acc_SoInvito.ORAAPP,0) + NVL(Acc_SoInvito.MINAPP,0)/60)/24 DTORAAPP,       
       SO_CENTRO_PREL_REF.DESCRBREVE,       
       SO_CNF_TPINVITO.IDTPINVITO || ' ' || SO_CNF_TPINVITO.DESCRIZIONE TIPO_INVITO,       
       SO_CNF_ESITOINVITO.DESCRSUGG DESCRSUGG,       
       SO_CNF_ESITOINVITO.METODO_CALCOLO METODO_CALCOLO,       
       SO_CNF_ESITOINVITO.DATA_OBBLIGATORIA DATA_OBBLIGATORIA,       
       Acc_SoInvito.DA_CONFERMARE,       
	Acc_SoInvito.TEST_PROPOSTO,       
	Acc_SoInvito.OTP       
FROM SO_INVITO Acc_SoInvito,SO_CENTRO_PREL_REF,SO_CNF_TPINVITO       
,SO_CNF_ESITOINVITO       
where SO_CENTRO_PREL_REF.IDCENTRO (+) = Acc_SoInvito.IDCENTROPRELIEVO       
and SO_CNF_TPINVITO.IDTPINVITO (+) = Acc_SoInvito.IDTPINVITO       
and SO_CNF_TPINVITO.ULSS (+) = Acc_SoInvito.ULSS       
and SO_CNF_TPINVITO.TPSCR (+) = Acc_SoInvito.TPSCR       
and SO_CNF_ESITOINVITO.CODESITOINVITO (+) = Acc_SoInvito.CODESITOINVITO and       
SO_CNF_ESITOINVITO.LIVESITO (+) = Acc_SoInvito.LIVESITO and       
SO_CNF_ESITOINVITO.ULSS (+) = Acc_SoInvito.ULSS and       
SO_CNF_ESITOINVITO.TPSCR (+) = Acc_SoInvito.TPSCR       
and Acc_SoInvito.IDINVITO=:1]]></SQLQuery>
