	SO_INTERVENTOCITO.N_INT
 FROM SO_INTERVENTOCITO , SO_CNF_REF2LIV ESEC,
 SO_CNF_REF2LIV INESEC, SO_CNF_REF2LIV TPINT,
 SO_CNF_REF2LIV ISTODIA,SO_OPMEDICO MEDICO_INT,
 SO_OPMEDICO MEDICO_RACC, SO_OPMEDICO MEDICO_IST,
 SO_CNF_SUGG_3LIV
 WHERE (SO_INTERVENTOCITO.OPZIONEESEC=ESEC.IDCNFREF2L(+) AND
 SO_INTERVENTOCITO.GROPZIONE=ESEC.GRUPPO(+) AND
 SO_INTERVENTOCITO.ULSS=ESEC.ULSS(+) AND
 SO_INTERVENTOCITO.TPSCR=ESEC.TPSCR(+))
 AND (SO_INTERVENTOCITO.MOTIVOESEC=INESEC.IDCNFREF2L(+) AND
 SO_INTERVENTOCITO.GRMOTIVO=INESEC.GRUPPO(+) AND
 SO_INTERVENTOCITO.ULSS=INESEC.ULSS(+) AND
 SO_INTERVENTOCITO.TPSCR=INESEC.TPSCR(+))
 AND (SO_INTERVENTOCITO.TPINTERVENTO=TPINT.IDCNFREF2L(+) AND
 SO_INTERVENTOCITO.GRTPINTERVENTO=TPINT.GRUPPO(+) AND
 SO_INTERVENTOCITO.ULSS=TPINT.ULSS(+) AND
 SO_INTERVENTOCITO.TPSCR=TPINT.TPSCR(+))
 AND (SO_INTERVENTOCITO.ISTDIA=ISTODIA.IDCNFREF2L(+) AND
 SO_INTERVENTOCITO.GRISTDIA=ISTODIA.GRUPPO(+) AND
 SO_INTERVENTOCITO.ULSS=ISTODIA.ULSS(+) AND
 SO_INTERVENTOCITO.TPSCR=ISTODIA.TPSCR(+))
 AND (SO_INTERVENTOCITO.MEDICOINT1=MEDICO_INT.IDOP(+))
 AND (SO_INTERVENTOCITO.MEDICORACC=MEDICO_RACC.IDOP(+))
 AND (SO_INTERVENTOCITO.IDMEDISTCHI=MEDICO_IST.IDOP(+))
 AND (SO_INTERVENTOCITO.IDSUGG=SO_CNF_SUGG_3LIV.IDSUGG3L(+) AND
 SO_INTERVENTOCITO.ULSS=SO_CNF_SUGG_3LIV.ULSS(+) AND
 SO_INTERVENTOCITO.TPSCR=SO_CNF_SUGG_3LIV.TPSCR(+))
) INTERVENTO

 
 where SO_INVITO.IDINVITO=SO_REFERTOCITO2LIV.IDINVITO
AND (SO_INVITO.CODTS=SO_SOGGETTO.CODTS AND SO_INVITO.ULSS=SO_SOGGETTO.ULSS)
AND SO_SOGGETTO.RELEASE_CODE_COM_SCR=SO_COMUNE.RELEASE_CODE(+)
AND SO_SOGGETTO.RELEASE_CODE_COM_DOM=SO_COMUNE1.RELEASE_CODE(+)
AND SO_SOGGETTO.RELEASE_CODE_COM_RES=SO_COMUNE2.RELEASE_CODE(+)
AND SO_SOGGETTO.CODDISTRZONA=SO_DISTRETTO_ZONA.CODDISTRZONA(+)
AND SO_SOGGETTO.ULSS=SO_DISTRETTO_ZONA.ULSS(+)
AND SO_INVITO.IDCENTROREF1LIV=SO_CENTRO_PREL_REF.IDCENTRO(+)
AND SO_REFERTOCITO2LIV.IDMEDCONCL=SO_OPMEDICO.IDOP(+)
AND  (SO_REFERTOCITO2LIV.IDSUGG=SO_CNF_SUGG_2LIV.IDSUGG2L(+) AND
SO_REFERTOCITO2LIV.ULSS=SO_CNF_SUGG_2LIV.ULSS(+) AND
SO_REFERTOCITO2LIV.TPSCR=SO_CNF_SUGG_2LIV.TPSCR(+))
AND SO_REFERTOCITO2LIV.IDREFERTO=COLPES.COLPES_IDREF(+)
AND SO_REFERTOCITO2LIV.IDREFERTO=BIODIA.BIODIA_IDREF(+)
AND S1.IDINVITO(+)=SO_REFERTOCITO2LIV.IDINVITO
AND SO_REFERTOCITO2LIV.IDREFERTO=INTERVENTO.IDREFERTO(+)
AND SO_REFERTOCITO2LIV.IDREFERTO=$P{id_referto}
ORDER BY INTERVENTO.N_INT]]></queryString>

