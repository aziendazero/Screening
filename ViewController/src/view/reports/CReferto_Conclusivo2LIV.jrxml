    SO_INTERVENTOCITO.DTINTERVENTO,
    SO_INTERVENTOCITO.MEDICOINT1,
    MEDICO_INT.COGNOME MINT_COGNOME,
    MEDICO_INT.NOME MINT_NOME,
    MEDICO_INT.TITOLO MINT_TITOLO,
