 FROM SO_INVITO, SO_REFERTOCITO2LIV, SO_SOGGETTO,
 SO_COMUNE, SO_COMUNE SO_COMUNE1, SO_COMUNE SO_COMUNE2,
 SO_DISTRETTO_ZONA,SO_CENTRO_PREL_REF, SO_OPMEDICO,
 SO_CNF_SUGG_2LIV,
 (SELECT
    SO_CODREF2LIVC2.IDREFERTO COLPES_IDREF,
    SO_CNF_REF2LIV.DESCRIZIONE COLPES_DESC
 FROM SO_CODREF2LIVC2, SO_CNF_REF2LIV
 WHERE (SO_CODREF2LIVC2.IDCNFREF=SO_CNF_REF2LIV.IDCNFREF2L AND
 SO_CODREF2LIVC2.GRUPPO=SO_CNF_REF2LIV.GRUPPO AND
 SO_CODREF2LIVC2.ULSS=SO_CNF_REF2LIV.ULSS AND
 SO_CODREF2LIVC2.TPSCR=SO_CNF_REF2LIV.TPSCR)
 AND SO_CODREF2LIVC2.GRUPPO='COLPES'
 ) COLPES,
 (SELECT
    SO_CODREF2LIVC2.IDREFERTO BIODIA_IDREF,
    SO_CNF_REF2LIV.DESCRIZIONE BIODIA_DESC
 FROM SO_CODREF2LIVC2, SO_CNF_REF2LIV
 WHERE (SO_CODREF2LIVC2.IDCNFREF=SO_CNF_REF2LIV.IDCNFREF2L AND
 SO_CODREF2LIVC2.GRUPPO=SO_CNF_REF2LIV.GRUPPO AND
 SO_CODREF2LIVC2.ULSS=SO_CNF_REF2LIV.ULSS AND
 SO_CODREF2LIVC2.TPSCR=SO_CNF_REF2LIV.TPSCR)
 AND SO_CODREF2LIVC2.GRUPPO='BIODIA'
 ) BIODIA,
 (SELECT S.IDINVITO ID_OLD,
S.DTAPP DTAPP_OLD,
SS.IDINVITO,
SS.DTAPP
 FROM SO_INVITO S,SO_INVITO SS
 WHERE (S.CODTS(+)=SS.CODTS AND S.ULSS(+)=SS.ULSS AND S.LIVELLO(+)=1 AND SS.LIVELLO=2)
 AND NOT EXISTS (SELECT 1 FROM SO_INVITO L2
WHERE L2.codts=SS.CODTS AND L2.ULSS=SS.ULSS AND L2.TPSCR=SS.TPSCR
AND L2.livello=1 AND L2.dtapp>S.DTAPP AND L2.dtapp<=SS.DTAPP)
) S1,
(SELECT
