    SO_REFERTOCITO2LIV.NOTECOLPOSCOPIA,
    SO_REFERTOCITO2LIV.IDMEDCOLPO,
    SO_REFERTOCITO2LIV.IDMEDCOLPO2,
    SO_REFERTOCITO2LIV.PRECITO,
    SO_REFERTOCITO2LIV.PREBIO,
    SO_REFERTOCITO2LIV.ISTBIOPTICA,
    SO_REFERTOCITO2LIV.DTBIOPSIA,
    SO_REFERTOCITO2LIV.NOTEBIOPSIA,
    SO_REFERTOCITO2LIV.IDMEDBIOPATO,
    SO_REFERTOCITO2LIV.IDMEDBIOPATO2,
    SO_REFERTOCITO2LIV.BIOHIV,
    SO_REFERTOCITO2LIV.BIOHPV,
    SO_REFERTOCITO2LIV.INTCHIUSI,
    SO_REFERTOCITO2LIV.TPREFERTO,
    SO_SOGGETTO.CODTS SOGG_CODTS,
    SO_SOGGETTO.CODDISTRZONA SOGG_CODDISTRZONA,
    SO_SOGGETTO.CF,
    SO_SOGGETTO.COGNOME,
    SO_SOGGETTO.NOME,
    SO_SOGGETTO.COGNOME_CONIUGE,
    SO_SOGGETTO.DATA_NASCITA,
    SO_SOGGETTO.SESSO,
    SO_SOGGETTO.TEL1,
    SO_SOGGETTO.TEL2,
    SO_SOGGETTO.ULSS SOGG_ULSS,
    DECODE(SO_SOGGETTO.CODCOMSCR,NULL,DECODE(SO_SOGGETTO.CODCOMDOM,NULL,SO_SOGGETTO.CODCOMRES,SO_SOGGETTO.CODCOMDOM),SO_SOGGETTO.CODCOMSCR) CODCOMUNE,
    DECODE(SO_SOGGETTO.CODCOMSCR,NULL,DECODE(SO_SOGGETTO.CODCOMDOM,NULL,SO_SOGGETTO.INDIRIZZO_RES,SO_SOGGETTO.INDIRIZZO_DOM),SO_SOGGETTO.INDIRIZZO_SCR) INDIRIZZO,
    DECODE(SO_SOGGETTO.CODCOMSCR,NULL,DECODE(SO_SOGGETTO.CODCOMDOM,NULL,SO_COMUNE2.DESCRIZIONE,SO_COMUNE1.DESCRIZIONE),SO_COMUNE.DESCRIZIONE) COMUNE,
    DECODE(SO_SOGGETTO.CODCOMSCR,NULL,DECODE(SO_SOGGETTO.CODCOMDOM,NULL,SO_COMUNE2.CODPR,SO_COMUNE1.CODPR),SO_COMUNE.CODPR) PROVINCIA,
    DECODE(SO_SOGGETTO.CODCOMSCR,NULL,DECODE(SO_SOGGETTO.CODCOMDOM,NULL,SO_COMUNE2.CAP,SO_COMUNE1.CAP),SO_COMUNE.CAP) CAP,
    SO_DISTRETTO_ZONA.CODDISTRZONA,
    SO_DISTRETTO_ZONA.DESCRIZIONE ZONA,
    SO_CENTRO_PREL_REF.IDCENTRO,
    SO_CENTRO_PREL_REF.DESCRIZIONE CENTRO_REF,
    SO_OPMEDICO.IDOP,
    SO_OPMEDICO.COGNOME COGNOME_MED,
